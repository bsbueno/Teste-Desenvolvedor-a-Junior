   "devDependencies": {
      "@types/node": "^8.0.29",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.1.6",
      "tsconfig-paths": "^3.9.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "body-parser": "^1.18.1",
      "concurrently": "^6.2.0",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "express": "^4.15.4",
